package com.meibaolian.entity;

/**
 *  煤种粒度实体
 *
 */
public class CoalParticleSize {
	
	private int id;
	
	private String name;
	
	private int type;
	
	
	public CoalParticleSize() {
	}

	public CoalParticleSize(int coalTypeId, String particleSizeName) {
		
		this.type=coalTypeId;
		this.name=particleSizeName;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	
	

}
